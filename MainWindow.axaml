<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="OldPhonePadApp.MainWindow"
        Title="Old Phone Pad Converter"
        Width="400" Height="200">
    <StackPanel>
        <TextBox Name="InputTextBox" Width="360" Margin="10"/>
        <Button Name="ConvertButton" Content="Convert" Width="100" Margin="10" HorizontalAlignment="Center"/>
        <TextBox Name="OutputTextBox" Width="360" Margin="10" IsReadOnly="True"/>
    </StackPanel>
</Window>
